{
  "name": "tms-dokumentarkiv",
  "version": "1.0.0",
  "description": "dokumentarkiv",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build --base=https://cdn.nav.no/min-side/tms-dokumentarkiv/ --outDir dist",
    "preview": "vite preview",
    "prepare": "husky install && chmod +x .husky/scripts/check-no-a11y-ignored.sh ",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "vitest"
  },
  "author": "navikt",
  "license": "MIT",
  "dependencies": {
    "@nanostores/react": "^0.7.1",
    "@navikt/aksel-icons": "^5.7.2",
    "@navikt/ds-react": "^5.7.2",
    "@navikt/ds-tokens": "^5.7.2",
    "@navikt/nav-dekoratoren-moduler": "^1.6.9",
    "amplitude-js": "^8.21.9",
    "react-router": "6.17.0",
    "react-router-dom": "6.17.0",
    "swr": "2.2.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@types/react-modal": "3.16.1",
    "@typescript-eslint/eslint-plugin": "6.8.0",
    "@vitejs/plugin-react": "4.1.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "@typescript-eslint/parser": "6.8.0",
    "husky": "8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "15.0.1",
    "msw": "^1.3.2",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup-plugin-import-map": "3.0.0",
    "rollup-plugin-terser": "7.0.2",
    "typescript": "5.2.2",
    "vite": "4.4.12",
    "vite-plugin-mock": "3.0.0",
    "vitest": "^0.34.6"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:@typescript-eslint/recommended",
      "plugin:jsx-a11y/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "@typescript-eslint/no-explicit-any": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{ts,tsx,css,md,html,json}": [
      "prettier --cache --write"
    ]
  },
  "engines": {
    "node": ">=18.15.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
